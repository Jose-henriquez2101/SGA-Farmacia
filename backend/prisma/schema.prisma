generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}

model Cliente {
  id        Int    @id @default(autoincrement())
  nombre    String
  rut       String @unique
  direccion String
  telefono  String
}

model Producto {
id Int @id @default(autoincrement())
nombre String
categoria String
cantidad Int
createdAt DateTime @default(now())
updatedAt DateTime @updatedAt
}
